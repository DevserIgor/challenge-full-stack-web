{
  "root": true,
  "env": {
    "node": true,
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:vue/vue3-essential",
    "eslint:recommended",
    "@vue/eslint-config-typescript"
  ],
  "rules": {
    "vue/multi-word-component-names": "off",
    "indent": ["error", 2],
    "comma-spacing": ["error", { "before": false, "after": true }],
    "vue/no-multi-spaces": 1
  }
}
